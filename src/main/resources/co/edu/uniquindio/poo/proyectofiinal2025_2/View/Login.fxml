<AnchorPane maxHeight="800.0" maxWidth="800.0" prefHeight="300.0" prefWidth="400.0" stylesheets="@../Style.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="co.edu.uniquindio.poo.proyectofiinal2025_2.Controller.LoginController">
